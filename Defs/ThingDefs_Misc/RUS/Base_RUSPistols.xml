<?xml version="1.0" encoding="utf-8" ?>

<Defs>

    <!-- PB -->

    <ThingDef ParentName="ERW_Pistol">
    <defName>ERW_Gun_PB_Pistol</defName>
    <label>PB</label>
    <description>The M4 carbine is a 5.56×45mm NATO, air-cooled, gas-operated,[b] magazine-fed, carbine, assault rifle developed in the United States during the 1980s. It is a shortened version of the M16A2 assault rifle.</description>
    <graphicData>
      <texPath>Things/Item/Equipment/Pistol/PB</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>Interact_Rifle</soundInteract>
    <recipeMaker>
      <researchPrerequisite>ERW_Weapons_RUSPistols</researchPrerequisite>
      <skillRequirements>
        <Crafting>6</Crafting>
      </skillRequirements>
    </recipeMaker>
	
	  <equippedAngleOffset>40</equippedAngleOffset>	
	
    <statBases>
      <WorkToMake>40000</WorkToMake>
      <Mass>3.5</Mass>
      <AccuracyTouch>0.57</AccuracyTouch>
      <AccuracyShort>0.60</AccuracyShort>
      <AccuracyMedium>0.72</AccuracyMedium>
      <AccuracyLong>0.65</AccuracyLong>
      <RangedWeapon_Cooldown>1.55</RangedWeapon_Cooldown>
    </statBases>
    <costList>
      <Steel>60</Steel>
      <ComponentIndustrial>7</ComponentIndustrial>
    </costList>
    <verbs>
      <li>
        <verbClass>Verb_Shoot</verbClass>
        <hasStandardCommand>true</hasStandardCommand>
        <defaultProjectile>ERW_Bullet_556x45_Rifle</defaultProjectile>
        <warmupTime>0.95</warmupTime>
        <range>37.9</range>
        <burstShotCount>1</burstShotCount>
        <ticksBetweenBurstShots>7</ticksBetweenBurstShots>
        <soundCast>ERW_Shot_PB</soundCast>
        <soundCastTail>GunTail_Medium</soundCastTail>
        <muzzleFlashScale>9</muzzleFlashScale>
      </li>
    </verbs>
		<weaponTags>
			<li>IndustrialGunAdvanced</li>
			<li>ERW_M4A1_MilSpec</li>
		</weaponTags>
    <tools>
      <li>
        <label>stock</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>9</power>
        <cooldownTime>2</cooldownTime>
      </li>
      <li>
        <label>barrel</label>
        <capacities>
          <li>Blunt</li>
          <li>Poke</li>
        </capacities>
        <power>9</power>
        <cooldownTime>2</cooldownTime>
      </li>
    </tools>
  </ThingDef>

