<?xml version="1.0" encoding="utf-8" ?>

<Defs>

    <!-- HK MP5K -->

    <ThingDef ParentName="ERW_SMG">
    <defName>ERW_Gun_HK_MP5K_SMG</defName>
    <label>HK MP5K</label>
    <description>Desc</description>
    <graphicData>
      <texPath>Things/Item/Equipment/SMG/PP_19_01</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>Interact_Rifle</soundInteract>
    <recipeMaker>
      <researchPrerequisite>ERW_Weapons_GERSmg</researchPrerequisite>
      <skillRequirements>
        <Crafting>7</Crafting>
      </skillRequirements>
    </recipeMaker>
	
	  <equippedAngleOffset>40</equippedAngleOffset>	
	
    <statBases>
      <WorkToMake>26000</WorkToMake>
      <Mass>3.1</Mass>
      <AccuracyTouch>0.81</AccuracyTouch>
      <AccuracyShort>0.60</AccuracyShort>
      <AccuracyMedium>0.25</AccuracyMedium>
      <AccuracyLong>0.20</AccuracyLong>
      <RangedWeapon_Cooldown>1.60</RangedWeapon_Cooldown>
    </statBases>
    <costList>
      <Steel>75</Steel>
      <ComponentIndustrial>5</ComponentIndustrial>
    </costList>
    <verbs>
      <li>
        <verbClass>Verb_Shoot</verbClass>
        <hasStandardCommand>true</hasStandardCommand>
        <defaultProjectile>ERW_Bullet_9x19_PP_SG</defaultProjectile>
        <warmupTime>0.90</warmupTime>
        <range>23.9</range>
        <burstShotCount>3</burstShotCount>
        <ticksBetweenBurstShots>10</ticksBetweenBurstShots>
        <soundCast>ERW_Shot_PP_19_01</soundCast>
        <soundCastTail>GunTail_Medium</soundCastTail>
        <muzzleFlashScale>9</muzzleFlashScale>
      </li>
    </verbs>
		<weaponTags>
			<li>IndustrialGunAdvanced</li>
			<li>ERW_M4A1_MilSpec</li>
		</weaponTags>
    <tools>
      <li>
        <label>stock</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>9</power>
        <cooldownTime>2</cooldownTime>
      </li>
      <li>
        <label>barrel</label>
        <capacities>
          <li>Blunt</li>
          <li>Poke</li>
        </capacities>
        <power>9</power>
        <cooldownTime>2</cooldownTime>
      </li>
    </tools>
    </ThingDef>

